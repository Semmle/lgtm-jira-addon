    <distributionManagement>
        <site>
            <id>atlassian-documentation</id>
            <url>scpexe://docs-app.internal.atlassian.com/var/www/domains/docs.atlassian.com/${project.artifactId}/${project.version}</url>
        </site>
    </distributionManagement>

    <reporting>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-javadoc-plugin</artifactId>
                <version>2.9.1</version>
            </plugin>
            <plugin>
               <groupId>org.apache.maven.plugins</groupId>
               <artifactId>maven-project-info-reports-plugin</artifactId>
               <reportSets>
                  <reportSet>
                      <reports>
                          <report>index</report>
                      </reports>
                  </reportSet>
               </reportSets>
            </plugin>
        </plugins>
    </reporting>

    <properties>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <springDmVersion>1.2.1</springDmVersion>
        <springVersion>2.5.6.SEC03</springVersion>
        <!-- Version for spring components patched for jdk8 : https://ecosystem.atlassian.net/browse/PLUG-1006-->
        <springJdk8Version>2.5.6.SEC03-atlassian-6</springJdk8Version>
        <slf4j.version>1.6.4</slf4j.version>
        <annotations.version>0.7</annotations.version>
        <felix.version>3.0.2</felix.version>
    </properties>
</project>

